{
    "name": "swdevelopment/animate",
    "description": "This will animate HTML elements using the AOS library.",
    "type": "craft-plugin",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "animate",
        "animation effects",
        "HTML animation"
    ],
    "version": "5.0.0",
    "support": {
        "docs": "https://github.com/sw-development-services/animate-plugin/blob/master/README.md",
        "issues": "https://github.com/sw-development-services/animate-plugin/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Tim Strawbridge",
            "homepage": "https://swdevteam.com"
        }
    ],
    "require": {
       "craftcms/cms": "^4.0|^5.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
          "swdevelopment\\animate\\": "src/"
        }
    },
    "extra": {
        "name": "Animate",
        "handle": "animate",
        "developer": "Tim Strawbridge",
        "developerUrl": "https://swdevteam.com",
        "documentationUrl": "https://github.com/sw-development-services/animate-plugin/blob/master/README.md",
        "changelogUrl": "https://github.com/sw-development-services/animate-plugin/blob/master/CHANGELOG.md",
        "class": "swdevelopment\\animate\\Animate"
    }
}
